{
    "$schema": "https://json-schema.org/draft-04/schema#",
    "name": "felipesantos2/slim",
    "description": "Tests with slim framework",
    "version": "1.0.0",
    "require": {
        "php": ">=8.3",
        "slim/slim": "^4.14",
        "league/plates": "*",
        "symfony/dotenv": "^7.2",
        "doctrine/orm": "^3",
        "doctrine/dbal": "^4",
        "symfony/cache": "^7"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/"
        },
        "files": [
            "app/helpers/helper.php"
        ]
    },
    "require-dev": {
        "symfony/var-dumper": "^7.2",
        "laravel/pint": "^1.22"
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "felipesantos2",
            "homepage": "https://github.com/felipesantos2",
            "role": "Developer"
        }
    ]
}
